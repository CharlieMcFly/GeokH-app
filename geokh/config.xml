<?xml version='1.0' encoding='utf-8'?>
<widget id="com.hubhouse.geokh" version="0.0.2" xmlns="http://www.w3.org/ns/widgets">
    <name>GeokH</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html"/>
    <access origin="*"/>

    <icon src="www/img/logo.png" platform="android" density="ldpi"/>
    <icon src="www/img/logo.png" platform="android" density="mdpi"/>
    <icon src="www/img/logo.png" platform="android" density="hdpi"/>
    <icon src="www/img/logo.png" platform="android" density="xhdpi"/>

    <preference name="orientation" value="portrait"/>


</widget>
